            <enum name="END" value="1"/>
        </attr>
        <attr name="cl_shapeType" format="enum">
            <enum name="CIRCULAR" value="0"/>
            <enum name="RECTANGULAR" value="1"/>
        </attr>
        <attr name="cl_placeholder" format="boolean"/>
        <attr name="cl_animatedContainer" format="boolean"/>
        <attr name="cl_clipCorners" format="boolean"/>
        <attr name="cl_animate" format="boolean"/>

        <attr name="cl_radius" format="dimension"/>
        <attr name="cl_topLeftCornerRadius" format="dimension" />
        <attr name="cl_topRightCornerRadius" format="dimension" />
        <attr name="cl_bottomLeftCornerRadius" format="dimension" />
        <attr name="cl_bottomRightCornerRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="FrameLayout">
        <attr name="fl_state" format="enum">
            <enum name="START" value="0"/>
            <enum name="END" value="1"/>
