            <enum name="HEIGHT" value="1"/>
            <enum name="NONE" value="-1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="TextLayout">
        <attr name="tl_state" format="enum">
            <enum name="START" value="0"/>
            <enum name="END" value="1"/>
        </attr>
        <attr name="tl_shapeType" format="enum">
            <enum name="CIRCULAR" value="0"/>
            <enum name="RECTANGULAR" value="1"/>
        </attr>
        <attr name="tl_animatedContainer" format="boolean"/>
        <attr name="tl_placeholder" format="boolean"/>
        <attr name="tl_animate" format="boolean"/>

        <attr name="tl_radius" format="dimension"/>
        <attr name="tl_topLeftCornerRadius" format="dimension" />
        <attr name="tl_topRightCornerRadius" format="dimension" />
        <attr name="tl_bottomLeftCornerRadius" format="dimension" />
        <attr name="tl_bottomRightCornerRadius" format="dimension" />
