<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MorphWrapper">
        <attr name="mw_startLayout" format="reference"/>
        <attr name="mw_endLayout" format="reference"/>
        <attr name="mw_shapeType" format="enum">
            <enum name="CIRCULAR" value="0"/>
            <enum name="RECTANGULAR" value="1"/>
        </attr>
        <attr name="mw_radius" format="dimension"/>
        <attr name="mw_topLeftCornerRadius" format="dimension" />
        <attr name="mw_topRightCornerRadius" format="dimension" />
        <attr name="mw_bottomLeftCornerRadius" format="dimension" />
        <attr name="mw_bottomRightCornerRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ConstraintLayout">
        <attr name="cl_state" format="enum">
            <enum name="START" value="0"/>
            <enum name="END" value="1"/>
        </attr>
        <attr name="cl_shapeType" format="enum">
            <enum name="CIRCULAR" value="0"/>
            <enum name="RECTANGULAR" value="1"/>
        </attr>
        <attr name="cl_radius" format="dimension"/>
        <attr name="cl_animate" format="boolean"/>
        <attr name="cl_topLeftCornerRadius" format="dimension" />
        <attr name="cl_topRightCornerRadius" format="dimension" />
        <attr name="cl_bottomLeftCornerRadius" format="dimension" />
        <attr name="cl_bottomRightCornerRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="FrameLayout">
        <attr name="fl_state" format="enum">
            <enum name="START" value="0"/>
            <enum name="END" value="1"/>
        </attr>
        <attr name="fl_shapeType" format="enum">
            <enum name="CIRCULAR" value="0"/>
            <enum name="RECTANGULAR" value="1"/>
        </attr>
        <attr name="fl_radius" format="dimension"/>
        <attr name="fl_animate" format="boolean"/>
        <attr name="fl_topLeftCornerRadius" format="dimension" />
        <attr name="fl_topRightCornerRadius" format="dimension" />
        <attr name="fl_bottomLeftCornerRadius" format="dimension" />
        <attr name="fl_bottomRightCornerRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="LinearLayout">
        <attr name="ll_state" format="enum">
            <enum name="START" value="0"/>
            <enum name="END" value="1"/>
        </attr>
        <attr name="ll_shapeType" format="enum">
            <enum name="CIRCULAR" value="0"/>
            <enum name="RECTANGULAR" value="1"/>
        </attr>
        <attr name="ll_radius" format="dimension"/>
        <attr name="ll_animate" format="boolean"/>
        <attr name="ll_topLeftCornerRadius" format="dimension" />
        <attr name="ll_topRightCornerRadius" format="dimension" />
        <attr name="ll_bottomLeftCornerRadius" format="dimension" />
        <attr name="ll_bottomRightCornerRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ViewLayout">
        <attr name="vl_state" format="enum">
            <enum name="START" value="0"/>
            <enum name="END" value="1"/>
        </attr>
        <attr name="vl_shapeType" format="enum">
            <enum name="CIRCULAR" value="0"/>
            <enum name="RECTANGULAR" value="1"/>
        </attr>
        <attr name="vl_radius" format="dimension"/>
        <attr name="vl_animate" format="boolean"/>
        <attr name="vl_topLeftCornerRadius" format="dimension" />
        <attr name="vl_topRightCornerRadius" format="dimension" />
        <attr name="vl_bottomLeftCornerRadius" format="dimension" />
        <attr name="vl_bottomRightCornerRadius" format="dimension" />
        <attr name="vl_snap_dimensions" format="enum">
            <enum name="WIDTH" value="0"/>
            <enum name="HEIGHT" value="1"/>
            <enum name="NONE" value="-1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="TextLayout">
        <attr name="tl_state" format="enum">
            <enum name="START" value="0"/>
            <enum name="END" value="1"/>
        </attr>
        <attr name="tl_shapeType" format="enum">
            <enum name="CIRCULAR" value="0"/>
            <enum name="RECTANGULAR" value="1"/>
        </attr>
        <attr name="tl_radius" format="dimension"/>
        <attr name="tl_animate" format="boolean"/>
        <attr name="tl_topLeftCornerRadius" format="dimension" />
        <attr name="tl_topRightCornerRadius" format="dimension" />
        <attr name="tl_bottomLeftCornerRadius" format="dimension" />
        <attr name="tl_bottomRightCornerRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="FabLayout"/>
</resources>