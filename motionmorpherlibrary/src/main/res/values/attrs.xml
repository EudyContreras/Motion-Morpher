            <enum name="CIRCULAR" value="0"/>
            <enum name="RECTANGULAR" value="1"/>
        </attr>

        <attr name="fl_placeholder" format="boolean"/>
        <attr name="fl_animatedContainer" format="boolean"/>
        <attr name="fl_clipCorners" format="boolean"/>
        <attr name="fl_animate" format="boolean"/>

        <attr name="fl_radius" format="dimension"/>
        <attr name="fl_topLeftCornerRadius" format="dimension" />
        <attr name="fl_topRightCornerRadius" format="dimension" />
        <attr name="fl_bottomLeftCornerRadius" format="dimension" />
        <attr name="fl_bottomRightCornerRadius" format="dimension" />
