<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MorphWrapper">
        <attr name="mw_startLayout" format="reference"/>
        <attr name="mw_endLayout" format="reference"/>
        <attr name="mw_shapeType" format="enum">
            <enum name="CIRCULAR" value="0"/>
            <enum name="RECTANGULAR" value="1"/>
        </attr>
        <attr name="mw_radius" format="dimension"/>
        <attr name="mw_topLeftCornerRadius" format="dimension" />
        <attr name="mw_topRightCornerRadius" format="dimension" />
        <attr name="mw_bottomLeftCornerRadius" format="dimension" />
        <attr name="mw_bottomRightCornerRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ViewGroup">
        <attr name="morphview_shapeType" format="enum">
            <enum name="CIRCULAR" value="0"/>
            <enum name="RECTANGULAR" value="1"/>
        </attr>
        <attr name="morphview_elementType" format="enum">
            <enum name="PERSISTENT" value="0"/>
            <enum name="INCOMING" value="1"/>
            <enum name="OUTGOING" value="2"/>
            <enum name="STATIC" value="3"/>
        </attr>
        <attr name="morphview_animatedContainer" format="boolean"/>
        <attr name="morphview_clipToOutline" format="boolean"/>
        <attr name="morphview_animated" format="boolean"/>

        <attr name="morphview_radius" format="dimension"/>
        <attr name="morphview_topLeftCornerRadius" format="dimension" />
        <attr name="morphview_topRightCornerRadius" format="dimension" />
        <attr name="morphview_bottomLeftCornerRadius" format="dimension" />
        <attr name="morphview_bottomRightCornerRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="View">
        <attr name="morphview_shapeType" format="enum">
            <enum name="CIRCULAR" value="0"/>
            <enum name="RECTANGULAR" value="1"/>
        </attr>
        <attr name="morphview_elementType" format="enum">
            <enum name="PERSISTENT" value="0"/>
            <enum name="INCOMING" value="1"/>
            <enum name="OUTGOING" value="2"/>
            <enum name="STATIC" value="3"/>
        </attr>
        <attr name="morphview_animated" format="boolean"/>

        <attr name="morphview_radius" format="dimension"/>
        <attr name="morphview_topLeftCornerRadius" format="dimension" />
        <attr name="morphview_topRightCornerRadius" format="dimension" />
        <attr name="morphview_bottomLeftCornerRadius" format="dimension" />
        <attr name="morphview_bottomRightCornerRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ConstraintLayout">
        <attr name="cl_state" format="enum">
