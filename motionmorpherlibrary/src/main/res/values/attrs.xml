    </attr>
    <attr name="il_animatedContainer" format="boolean"/>
    <attr name="il_placeholder" format="boolean"/>
    <attr name="il_animate" format="boolean"/>

    <attr name="il_radius" format="dimension"/>
    <attr name="il_topLeftCornerRadius" format="dimension" />
    <attr name="il_topRightCornerRadius" format="dimension" />
    <attr name="il_bottomLeftCornerRadius" format="dimension" />
    <attr name="il_bottomRightCornerRadius" format="dimension" />
</declare-styleable>

    <declare-styleable name="FabLayout"/>

    <declare-styleable name="RoundedImageView">
        <attr name="riv_radius" format="dimension"/>
        <attr name="riv_topLeftCornerRadius" format="dimension" />
        <attr name="riv_topRightCornerRadius" format="dimension" />
        <attr name="riv_bottomLeftCornerRadius" format="dimension" />
        <attr name="riv_bottomRightCornerRadius" format="dimension" />
    </declare-styleable>

</resources>